int isPrime(int num) {
    int i = 2;
    if (num <= 1) {
        return 0;
    }
    
    while (i * i <= num) {
        if (num - (num / i) * i == 0) {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}

int main() {
    int number;
    number = read();
    if (isPrime(number) == 1) {
        write(1);
    } else {
        write(0);
    }
    return 0;
}
